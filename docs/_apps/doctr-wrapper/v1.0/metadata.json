{
  "name": "docTR Wrapper",
  "description": "End to end OCR for extracting text from timeframes",
  "app_version": "v1.0",
  "mmif_version": "1.0.2",
  "analyzer_version": "0.8.1",
  "app_license": "MIT",
  "analyzer_license": "Apache 2.0",
  "identifier": "http://apps.clams.ai/doctr-wrapper/v1.0",
  "url": "https://github.com/clamsproject/app-easyocr-wrapper",
  "input": [
    {
      "@type": "http://mmif.clams.ai/vocabulary/VideoDocument/v1",
      "required": true
    },
    {
      "@type": "http://mmif.clams.ai/vocabulary/TimeFrame/v3",
      "required": true
    }
  ],
  "output": [
    {
      "@type": "http://mmif.clams.ai/vocabulary/TextDocument/v1"
    },
    {
      "@type": "http://vocab.lappsgrid.org/Sentence"
    },
    {
      "@type": "http://vocab.lappsgrid.org/Paragraph"
    },
    {
      "@type": "http://vocab.lappsgrid.org/Token"
    },
    {
      "@type": "http://mmif.clams.ai/vocabulary/Alignment/v1"
    },
    {
      "@type": "http://mmif.clams.ai/vocabulary/BoundingBox/v2"
    }
  ],
  "parameters": [
    {
      "name": "pretty",
      "description": "The JSON body of the HTTP response will be re-formatted with 2-space indentation",
      "type": "boolean",
      "default": 0,
      "multivalued": false
    }
  ]
}